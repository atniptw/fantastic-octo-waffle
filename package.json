{
  "name": "fantastic-octo-waffle",
  "version": "0.1.0",
  "private": true,
  "description": "A static web app for browsing and rendering cosmetics from REPO game mods",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "test:unit": "pnpm -r run test:unit",
    "test:integration": "pnpm -r run test:integration",
    "test:e2e": "pnpm -r run test:e2e",
    "test:watch": "pnpm -r run test:watch",
    "test:cov": "pnpm -r run test:cov",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "typecheck": "pnpm -r run typecheck",
    "deadcode": "ts-prune --error",
    "clean": "pnpm -r run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.4.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "unity",
    "assetbundle",
    "parser",
    "three.js",
    "preact",
    "cloudflare-workers",
    "monorepo"
  ],
  "author": "",
  "license": "MIT"
}
