{
  "name": "repo-cosmetic-catalog",
  "version": "1.0.0",
  "type": "module",
  "description": "A browser-based tool for previewing R.E.P.O. cosmetic mods with 3D rendering",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "keywords": [
    "browser",
    "cosmetic",
    "catalog",
    "r.e.p.o.",
    "mods",
    "unity",
    "3d-preview"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.170.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.12.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "gif.js": "^0.2.0",
    "idb": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sevenzip-wasm": "^0.1.8",
    "three": "^0.170.0"
  }
}
